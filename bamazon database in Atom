CREATE DATABASE bamazonDB;

USE bamazonDB;

CREATE TABLE "products" (
'itemid' INT NOT NULL AUTO_INCREMENT,
"product" VARCHAR(50) NULL,
"department" VARCHAR (30) NULL,
"price" DECIMAL (10,2) NULL,
"quantity" INT  NULL,
PRIMARY KEY ("itemid")

);

